package q11046;

import java.util.Arrays;

public class SumOfElements {
    /**
     * Computes the sum of all the elements in the
     * given array
     *
     *
     * @return the sum
     */
    public long sum(int[] arr) {
        // Write the code
        long k = 0;
        for (int s : arr) {
            k += s;
        }
        if (arr.length > 0) {
            return k;
        } else {
            return 0;
        }
    }
}
