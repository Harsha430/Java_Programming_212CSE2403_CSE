package q11047;

public class ReversePrint {
    /**
     * write a logic to find the array of elements
     * in reverse order.
     *
     *
     * @return the reverseArray
     */
    public int[] reversePrint(int[] arr) {
        int[] n = new int[arr.length];
        int a = arr.length - 1;
        for (int n1 : arr) {
            n[a] = n1;
            a--;
        }
        return n;
    }
}
