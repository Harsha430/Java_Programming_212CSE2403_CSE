package q11075;

public class CountOfTwoNumbers {
    /**
     * Find the count of arg1 is more than arg2 in
     * the arr or not
     *
     *
     *
     * @return result
     */
    public boolean compareCountOf(int[] arr, int arg1, int arg2) {
        int count1 = 0, count2 = 0;
        for (int n : arr) {
            if (n == arg1) {
                count1++;
            }
            if (n == arg2) {
                count2++;
            }
        }
        if (count1 > count2) {
            return true;
        }
        return false;
    }
}
