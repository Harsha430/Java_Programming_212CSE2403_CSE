package q11184;

public class PalindromeOrNot {
    public static void main(String[] args) {
        String str = "";
        for (int i = args[0].length() - 1; i >= 0; i--) {
            str += args[0].charAt(i);
        }
        if (str.equals(args[0])) {
            System.out.println("The given string " + args[0] + " is a palindrome");
        } else {
            System.out.println("The given string " + args[0] + " is not a palindrome");
        }
    }
}
