package q10885;

public class PrimeVerify {
    public void checkPrimeOrNot(int num) {
        int c = 0;
        for (int i = 2; i <= num; i++) {
            if (num % i == 0) {
                c++;
            }
        }
        if (num == 1) {
            System.out.println(num + " is not a prime number");
        }
        if (c > 1) {
            System.out.println(num + " is not a prime number");
        } else {
            System.out.println(num + " is a prime number");
        }
    }
}
