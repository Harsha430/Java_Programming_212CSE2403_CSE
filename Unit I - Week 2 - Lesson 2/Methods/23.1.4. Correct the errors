package q11128;

public class Student {
    private String id;
    private String name;
    private int age;
    private char gender;

    public Student(String name, String id, int age, char gender) {
        this.id = id;
        this.name = name;
        this.age = age;
        this.gender = gender;
    }

    public String toString() {
        return "Student [name = " + name + ", id = " + id +
                ", age = " + age + ", gender = " + gender + "]";
    }
}
