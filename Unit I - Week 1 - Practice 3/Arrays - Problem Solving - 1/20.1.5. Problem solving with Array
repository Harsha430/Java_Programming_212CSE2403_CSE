package q11050;

public class ElementCheck {
    /**
     * check if first or last elements of the arr1
     * and arr2 are same or not
     *
     *
     * @return result
     */
    public boolean checkFirstOrLast(int[] arr1,
            int[] arr2) {
        if (arr1[0] == arr2[0] || arr1[arr1.length1] == arr2[arr2.length - 1]) {
            return true;
        }
        return false;
    }
}
