package q11209;

public class CheckChars {
    public static void main(String[] args) {
        int count = 0;
        String s = args[0];
        if (s.contains("w") && s.contains("e") &&
                s.contains("b")) {
            System.out.println(true);
        } else {
            System.out.println(false);
        }
    }
}
