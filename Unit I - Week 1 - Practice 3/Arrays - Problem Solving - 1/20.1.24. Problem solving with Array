package q11072;

import java.util.Arrays;

public class FindAverage {
    /**
     * Compute the average of elements in the given
     * array
     *
     *
     * @return result
     */
    public int findAverage(int[] arr) {
        // Write your code here
        int sum = Arrays.stream(arr).sum();
        return sum / arr.length;
    }
}
