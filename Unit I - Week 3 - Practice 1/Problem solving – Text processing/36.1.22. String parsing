package q11212;

public class RemoveSuffix {
    public static void main(String[] args) {
        String s = args[0];
        String s1 = s.substring(0, 3);
        if (s.endsWith(s1)) {
            System.out.println(s.substring(0, s.length() - 3));
        } else {
            System.out.println(s);
        }
    }
}
